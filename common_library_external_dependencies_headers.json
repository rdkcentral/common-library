{
  "dependencies": [
    {
      "name": "rdk_logger",
      "repository": "https://github.com/rdkcentral/rdk_logger.git",
      "branch": "main",
      "header_paths": [
        {
          "source": "include",
          "destination": "rdk_logger"
        }
      ]
    },
    {
      "name": "trower-base64",
      "repository": "https://github.com/xmidt-org/trower-base64.git",
      "branch": "main",
      "header_paths": [
        {
          "source": "include/trower-base64",
          "destination": "trower-base64"
        }
      ]
    },
    {
      "name": "rbus",
      "repository": "https://github.com/rdkcentral/rbus.git",
      "branch": "main",
      "header_paths": [
        {
          "source": "include",
          "destination": "rbus"
        },
        {
          "source": "src/rbus",
          "destination": "rbus"
        },
        {
          "source": "src/core",
          "destination": "rbus"
        },
        {
          "source": "src/rtmessage",
          "destination": "rtmessage"
        }
      ]
    }
  ]
}
